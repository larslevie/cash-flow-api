{
  consul: "{{ .CONSUL_URL }}:8500",
  jobs: [
    {
      name: "cash-flow-api",
      exec: ["bundle", "exec", "rails", "s", "Puma", "-b", "0.0.0.0"],
      port: 3000,
      tags: ["consul-lb.url=cashflowapi.com.dev"],
      health: {
        exec: "/usr/bin/curl -o /dev/null --fail -s http://localhost:3000/health_check",
        interval: 5,
        ttl: 10,
        timeout: "5s"
      },
    },
  ],
}
